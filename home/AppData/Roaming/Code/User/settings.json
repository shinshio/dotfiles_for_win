{
    "workbench.startupEditor": "newUntitledFile",
    "workbench.iconTheme": "vscode-icons",
    "files.insertFinalNewline": true,
    "workbench.editor.closeEmptyGroups": false,
    "breadcrumbs.enabled": true,
    "vsicons.dontShowNewVersionMessage": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    // https://python-climbing.com/vscode-auto-off/
    "typescript.tsc.autoDetect": "off",
    "grunt.autoDetect": "off",
    "jake.autoDetect": "off",
    "npm.autoDetect": "off",
    "gulp.autoDetect": "off",
    "python.jediEnabled": false,
    "C_Cpp.updateChannel": "Insiders",
    // arduino
    "arduino.path": "C:/Program Files (x86)/Arduino",
    "C_Cpp.intelliSenseEngine": "Tag Parser",
    "files.trimFinalNewlines": true,
    "window.openFilesInNewWindow": "on",
    // open file at new tab
    "workbench.editor.enablePreview": false,
    // python import error
    "python.envFile": "${workspaceFolder}/.env",
    "editor.largeFileOptimizations": false,
    "files.eol": "\r\n",
    "http.proxyStrictSSL": false,
    "extensions.ignoreRecommendations": false,
    "extensions.autoUpdate": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.minimap.maxColumn": 200,
    "python.languageServer": "Pylance",
    "platformio-ide.disablePIOHomeStartup": true,
    "python.showStartPage": false,
    "python.venvFolders": [
        ".venv"
    ],
    "python.venvPath": ".venv,",
    "editor.rulers": [
        80
    ],
    "markdown.extension.list.indentationSize": "inherit",
    "markdown-pdf.margin.left": "0cm",
    "markdown-pdf.margin.right": "0cm",
    "markdown-pdf.margin.bottom": "0cm",
    "markdown-pdf.displayHeaderFooter": false,
    "markdown-pdf.margin.top": "0cm",
    "editor.fontFamily": "'HackGen35Nerd Console', Consolas, 'Courier New', monospace",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.tabSize": 2
    },
    "[typescriptreact]": {
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.tabSize": 2,
        "editor.suggest.insertMode": "replace",
        "gitlens.codeLens.scopes": [
            "document"
        ]
    },
    "[powershell]": {
        "files.encoding": "shiftjis"
    },
    "files.associations": {
        "*.dcm": "vba"
    },
    "[vba]": {
        "editor.tabSize": 4,
        "files.encoding": "shiftjis"
    },
    "jupyter.interactiveWindowMode": "perFile",
    "editor.minimap.enabled": false,
    "files.autoGuessEncoding": true,
    "workbench.colorTheme": "Monokai",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "editor.formatOnSave": true,
    "python.linting.flake8Args": [
        "--ignore=E501, W503"
    ],
    "markdown-pdf.highlightStyle": "github.css",
    "workbench.editor.untitled.hint": "hidden",
    "vscode-neovim.neovimExecutablePaths.win32": "C:/tools/Neovim/bin/nvim.exe",
    "vscode-neovim.neovimInitVimPaths.win32": "C:\\Users\\10035637417\\.config\\nvim\\init.vim",
    "vscode-neovim.mouseSelectionStartVisualMode": true,
    "security.workspace.trust.untrustedFiles": "open",
}
